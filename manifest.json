{
  "manifest_version": 2,

  "name": "NYT Truisms",
  "description": "Replaces random NYT article headlines with Jenny Holzer's truisms",
  "version": "1.0",
  "icons": {"128":"flash.png"},

  "content_scripts": [
    {
      "matches":["https://*.nytimes.com/*"],
      "js": ["myscript.js"],
      "run_at": "document_end"
    }
  ]
}
